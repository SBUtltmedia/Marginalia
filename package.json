{
  "name": "Marginalia",
  "version": "1.0.0",
  "description": "An implentation of Socket.io to set an entities attributes",
  "main": "index.js",
  "unpkg": "dist/aframe-websocket-component.min.js",
  "scripts": {
    "build": "webpack js/index.js dist/bundle.js",
    "dev": "nodemon --watch script --watch js --exec 'npm run dist' ",
    "dist": "npm run build && uglifyjs dist/bundle.js > dist/bundle.min.js",
    "lint": "semistandard -v | snazzy",
    "prepublish": "npm run dist",
    "ghpages": "ghpages",
    "start": "npm run dev",
    "test": "karma start ./tests/karma.conf.js",
    "test:firefox": "karma start ./tests/karma.conf.js --browsers Firefox",
    "test:chrome": "karma start ./tests/karma.conf.js --browsers Chrome"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/https://github.com/SBUtltmedia/aframe-websocket-component.git"
  },
  "keywords": [
    "aframe",
    "aframe-component",
    "aframe-vr",
    "vr",
    "webvr",
    "websocket"
  ],
  "author": "TLTMediaLab@connect.stonybrook.edu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/https://github.com/SBUtltmedia/aframe-websocket-component/issues"
  },
  "homepage": "https://github.com/https://github.com/SBUtltmedia/aframe-websocket-component#readme",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "aframe": "*",
    "babel-loader": "^8.0.5",
    "babel-preset": "^1.0.6",
    "babel-preset-env": "^1.7.0",
    "browserify": "^13.0.0",
    "budo": "^8.2.2",
    "chai": "^3.4.1",
    "chai-shallow-deep-equal": "^1.3.0",
    "ghpages": "^0.0.8",
    "karma": "^0.13.15",
    "karma-browserify": "^4.4.2",
    "karma-chai-shallow-deep-equal": "0.0.4",
    "karma-chrome-launcher": "2.0.0",
    "karma-env-preprocessor": "^0.1.1",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.3",
    "karma-sinon-chai": "^1.1.0",
    "mocha": "^2.3.4",
    "nodemon": "^1.18.10",
    "randomcolor": "^0.4.4",
    "semistandard": "^8.0.0",
    "shelljs": "^0.7.0",
    "shx": "^0.1.1",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "snazzy": "^4.0.0",
    "uglify-es": "^3.3.9",
    "webpack": "^2.7.0"
  },
  "semistandard": {
    "globals": [
      "AFRAME",
      "THREE"
    ],
    "ignore": [
      "examples/build.js",
      "dist/**"
    ]
  },
  "dependencies": {
    "express": "^4.16.4",
    "socket.io": "*"
  }
}
